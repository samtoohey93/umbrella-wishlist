{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "umbrella-wishlist/umbrella-wishlist",
	"version": "0.1.0",
	"title": "Umbrella Wishlist",
	"category": "media",
	"icon": "smiley",
	"description": "A Wishlist Plugin for the Umbrella Theme",
	"supports": {
		"html": false
	},
	"textdomain": "bumberlist",
	"styles": [
		{
			"name": "default",
			"label": "Default",
			"isDefault": true
		},
		{
			"name": "outline",
			"label": "Outline"
		},
		{
			"name": "rounded",
			"label": "Rounded"
		}
	],
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		},
		"linkText": {
			"type": "string"
		},
		"align": {
			"type": "string",
			"default": "none"
		},
		"backgroundColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"affiliateLink": {
			"type": "string",
			"default": ""
		},
		"linkLabel": {
			"type": "string",
			"default": "Check it out!"
		},
		"hasLinkNofollow": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
